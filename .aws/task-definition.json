{
  "family": "student-performance-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::040291911741:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",

  "containerDefinitions": [
    {
      "name": "student-performance-container",
      "image": "040291911741.dkr.ecr.us-east-1.amazonaws.com/student-performance:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/student-performance",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
